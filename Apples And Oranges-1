# include<stdio.h>
int main()
{
    int s,t;
    scanf("%d%d",&s,&t);
    int a,b;
     scanf("%d%d",&a,&b);
     int m,n;
      scanf("%d%d",&m,&n);
      int d,ac=0,oc=0;
      for(int i=1; i<=m; i++)
      {
          scanf("%d",&d);
          if(a+d>=s && a+d<=t)
          ac++;
      }
      for(int j=1; j<=n; j++)
      {
          scanf("%d",&d);
          if(b+d>=s && b+d<=t)
          oc++;
      }
      printf("%d\n%d",ac,oc);
    
}
